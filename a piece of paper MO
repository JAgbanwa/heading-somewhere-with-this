\begin{align*}
     6 \cdot n &= x+y+z \\
     6 \cdot n  &= 870406166 + 8762318 + (-870406462)\\
     n &= 1460337
\end{align*}

\begin{equation*}
    x_1 = -\alpha + \sqrt{(6n + \alpha)^2 + \frac{36n^3 - 100}{\alpha}} 
\end{equation*}

\begin{equation*}
    y_1 = 2 \cdot \alpha + 6n 
\end{equation*}

\begin{equation*}
    z_1 = -\alpha - \sqrt{(6n  + \alpha)^2 + \frac{36n^3 - 100}{\alpha}} 
\end{equation*}

